<?xml version='1.0' encoding='utf-8'?><widget id="eu.jmorille.nfcmim" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" ios-CFBundleVersion="1">
    <name>Nfc MiM</name>
    <description>Nfc Man In The Middle</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Author Name &lt;a@b.com></author>
    <content src="plugins/cordova-plugin-chrome-apps-bootstrap/chromeapp.html"/>
    <plugin name="cordova-plugin-whitelist" version="1"/>
    <plugin name="com.chariotsolutions.nfc.plugin" spec="^0.6.2" />

    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
        <icon src="www/assets/icon_128.png"/><icon src="www/assets/icon_128.png" width="128"/>
    </platform>
<preference name="android-minSdkVersion" value="18"/></widget>